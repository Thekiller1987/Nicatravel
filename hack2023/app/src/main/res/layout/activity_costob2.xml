<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mun"
    tools:context=".costob_activity">

  <TextView
      android:id="@+id/textViewCosto"
      android:layout_width="141dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="68dp"
      android:background="#00000000"
      android:backgroundTint="#000000"
      android:foregroundTint="#000000"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/textViewNombre"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="16dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/textViewCosto" />

  <TextView
      android:id="@+id/textViewCosto2"
      android:layout_width="141dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="124dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/textViewNombre" />

  <TextView
      android:id="@+id/textViewNombre2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="16dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/textViewCosto2" />

  <TextView
      android:id="@+id/textViewCosto3"
      android:layout_width="141dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="176dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/textViewNombre2" />

  <TextView
      android:id="@+id/textViewNombre3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="16dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/textViewCosto3" />

  <TextView
      android:id="@+id/textViewCosto4"
      android:layout_width="141dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="152dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/textViewNombre3" />

  <TextView
      android:id="@+id/textViewNombre4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="16dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/textViewCosto4" />

  <ImageView
      android:id="@+id/imageView"
      android:layout_width="104dp"
      android:layout_height="90dp"
      android:layout_marginTop="52dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textViewCosto"
      app:srcCompat="@drawable/lcab" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="254dp"
      android:layout_height="238dp"
      app:layout_constraintTop_toBottomOf="@+id/textViewCosto2"
      app:srcCompat="@drawable/lcde"
      tools:layout_editor_absoluteX="-15dp" />

  <ImageView
      android:id="@+id/imageView9"
      android:layout_width="221dp"
      android:layout_height="131dp"
      android:layout_marginTop="48dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textViewCosto3"
      app:srcCompat="@drawable/gallo" />

  <ImageView
      android:id="@+id/imageView10"
      android:layout_width="223dp"
      android:layout_height="83dp"
      android:layout_marginTop="48dp"
      app:layout_constraintStart_toStartOf="@+id/textViewNombre4"
      app:layout_constraintTop_toBottomOf="@+id/textViewCosto4"
      app:srcCompat="@drawable/bigoron" />

  <Button
      android:id="@+id/btnagregar"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Agregar"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.479"
      app:layout_constraintStart_toEndOf="@+id/imageView"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.208" />

  <Button
      android:id="@+id/btnavion"
      android:layout_width="66dp"
      android:layout_height="64dp"
      android:background="@drawable/avion"
      tools:layout_editor_absoluteX="343dp"
      tools:layout_editor_absoluteY="-2dp" />

  <TextView
      android:id="@+id/suma"
      android:layout_width="93dp"
      android:layout_height="21dp"
      android:text=""
      tools:layout_editor_absoluteX="159dp"
      tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
